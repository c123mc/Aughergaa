<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".LandingActivity"
    tools:showIn="@layout/app_bar_landing">

    <RelativeLayout
        android:layout_width="match_parent"
        android:id="@+id/other_frams"
        android:layout_height="match_parent">
    <fragment
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        class="com.conor.aughergaa.Fragments.HomeFragment"
        android:name="com.conor.aughergaa.Fragments.HomeFragment"
        android:id="@+id/fram"
        />
    </RelativeLayout>

    <RelativeLayout
        android:layout_width="match_parent"
        android:id="@+id/maps_layout"
        android:layout_height="match_parent">

        <fragment
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:id="@+id/map"
            tools:context=".LandingActivity"
            android:name="com.google.android.gms.maps.SupportMapFragment" />

        <Button
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_margin="24dp"
            android:layout_alignParentBottom="true"
            android:background="@color/primary"
            android:textColor="@android:color/white"
            android:text="Send us your query"
            android:id="@+id/start_contact"
            />

    </RelativeLayout>

</RelativeLayout>